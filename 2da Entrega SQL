# MOODIFICACIONES DE LAS TABLAS
# TABLA PROVEEDORES
	ALTER TABLE proveedores
	ADD FOREIGN KEY(localidad) REFERENCES localidades(id_localidad);
    
    ALTER TABLE proveedores
    ADD FOREIGN KEY(insumos) REFERENCES insumos(id_insumos);

# TABLA INSUMOS
	ALTER TABLE insumos
	ADD FOREIGN KEY(sector) REFERENCES sectores(id_sector);
    
# TABLA VENTAS
	ALTER TABLE ventas
	ADD FOREIGN KEY(clientes) REFERENCES clientes(id_clientes);

	ALTER TABLE ventas
	ADD FOREIGN KEY(productos) REFERENCES productos(id_productos);

	ALTER TABLE ventas
	ADD FOREIGN KEY(stock) REFERENCES stock(id_stock);
    
    ALTER TABLE ventas
    ADD FOREIGN KEY(viajantes) REFERENCES viajantes(id_viajante);
       
# TABLA FACTURACIÓN_VENTAS
	ALTER TABLE facturación_ventas
	ADD FOREIGN KEY(cliente) REFERENCES clientes(id_clientes);

# TABLA FACTURACIÓN_PROVEEDORES
	ALTER TABLE facturación_proveedores
	ADD FOREIGN KEY(proveedor) REFERENCES proveedores(id_proveedores);

# TABLA FACTURACIÓN_APARADORES
	ALTER TABLE facturación_aparadores
    ADD FOREIGN KEY(aparador) REFERENCES aparadores(id_aparador);

# TABLA EMPLEADOS
	ALTER TABLE empleados
	ADD FOREIGN KEY(localidad) REFERENCES localidades(id_localidad);    
    
	ALTER TABLE empleados
	ADD FOREIGN KEY(sector) REFERENCES sectores(id_sector);
    
    UPDATE empleados
    SET dni = 24284115
    WHERE id_empleados = 19;
    
# TABLA PRODUCTOS
	ALTER TABLE productos
	ADD FOREIGN KEY(denominación) REFERENCES denominaciones(id_denominación);
    
    ALTER TABLE productos
    ADD FOREIGN KEY(artículo) REFERENCES artículos(id_artículo);
    
# TABLA CLIENTES
	ALTER TABLE clientes
	ADD FOREIGN KEY(localidad) REFERENCES localidades(id_localidad);
    
    ALTER TABLE clientes
    ADD FOREIGN KEY(viajante) REFERENCES viajantes(id_viajante);
    
# TABLA APARADORES
	ALTER TABLE aparadores
	ADD FOREIGN KEY(localidad) REFERENCES localidades(id_localidad);

	ALTER TABLE aparadores
    ADD FOREIGN KEY(insumos) REFERENCES insumos(id_insumos);
    
# TABLA VIAJANTES
	ALTER TABLE viajantes
	ADD FOREIGN KEY(localidad) REFERENCES localidades(id_localidad);
    
	ALTER TABLE viajantes
	ADD FOREIGN KEY(zona) REFERENCES zonas(id_zona);
    
# TABLA LOCALIDADES
	ALTER TABLE localidades
    ADD FOREIGN KEY(provincia) REFERENCES provincia(id_provincia);
    
# TABLA TALLES
	ALTER TABLE talles
	MODIFY COLUMN número VARCHAR(30) NOT NULL;
    
# TABLA ARTÍCULOS    
	ALTER TABLE artículos
    ADD FOREIGN KEY(talle) REFERENCES talles(id_talle);

# CREACIÓN DE VISTAS    
# 1- NOMBRE Y APELLIDO DE CLIENTES QUE REGISTRAN DEUDAS
CREATE OR REPLACE VIEW clientes_morosos AS
(SELECT c.nombre, c.apellido, c.teléfono, c.email, c.localidad, fv.composición_de_deuda
FROM clientes c 
JOIN facturación_ventas fv ON (c.id_clientes = fv.cliente)
WHERE fv.composición_de_deuda != 0
);

SELECT * FROM clientes_morosos;

# 2 - NOMBRE Y APELLIDO DE EMPLEADOS CON UN SALARIO MENOR A $100.000
CREATE OR REPLACE VIEW empleados_100k AS
(SELECT nombre, apellido, salario, cuit, fecha_nacimiento, sector, localidad
FROM empleados
WHERE salario < 100000
);

SELECT * FROM empleados_100k;

# 3 - PRODUCTOS QUE CONTENGAN 'DINO' EN SU DENOMINACIÓN
CREATE OR REPLACE VIEW productos_dino AS
(SELECT * FROM denominaciones
WHERE nombre like 'Dino%'
);

SELECT * FROM productos_dino;

# 4 - MONTO VENDIDO DE LOS PRODUCTOS CON FALLAS
CREATE OR REPLACE VIEW monto_productos_fallas AS
 (SELECT v.id_ventas, v.fecha, v.monto, v.productos
 FROM ventas v
 LEFT JOIN stock s ON (v.stock = s.id_stock)
 WHERE id_stock = 3
 );
 
 SELECT * FROM monto_productos_fallas;

# 5 - VENTAS POR ZONA
CREATE OR REPLACE VIEW ventas_x_zona AS
(SELECT ve.id_ventas, ve.fecha, ve.monto, ve.cantidad, ve.clientes id_cliente, vi.nombre nombre_viajante, vi.apellido apellido_viajante, z.nombre zona
FROM ventas ve
JOIN viajantes vi ON (ve.viajantes = vi.id_viajante)
JOIN zonas z ON (vi.zona = z.id_zona)
WHERE ve.monto != 0
);

SELECT * FROM ventas_x_zona;

# 6 - TOP 10 PRODUCTOS MÁS VENDIDOS
CREATE OR REPLACE VIEW top10_productos AS
(SELECT p.id_productos, v.monto
FROM productos p
JOIN ventas v ON (p.id_productos = v.productos)
ORDER BY monto DESC
LIMIT 10
);

SELECT * FROM top10_productos;

# 7 - NOMBRE Y APELLIDO DE LOS VIAJANTES QUE HAYAN LOGRADO VENTAS MAYORES O IGUALES A $500.000
CREATE OR REPLACE VIEW ventas_mayores_500k AS
	(SELECT vi.nombre, vi.apellido, ve.fecha, ve.monto, ve.cantidad, ve.clientes, ve.productos
     FROM ventas ve
     INNER JOIN viajantes vi ON (ve.viajantes = vi.id_viajante)
     WHERE ve.monto >= 500000
     );

SELECT * FROM ventas_mayores_500k;

# CREACIÓN DE FUNCIONES
# 1 - DETERMINAR LA CATEGORÍA DEL SALARIO EN "BAJO", "MEDIO" O "ALTO", SEGÚN CUMPLA X VALOR EN $.
DELIMITER $$
CREATE FUNCTION `tipo_salario`(salario INT) RETURNS VARCHAR(30)
DETERMINISTIC
BEGIN
DECLARE tipo VARCHAR(30);

CASE
	WHEN salario < 115000 THEN
    SET tipo = 'bajo';
	WHEN salario < 150000 THEN
	SET tipo = 'medio';
	ELSE
    SET tipo = 'alto';
END CASE;

RETURN tipo;
END
$$

DELIMITER ;
SELECT purinky_dw.tipo_salario(170000) AS Tipo_de_salario;

# 2 - CONTAR LOS PRODUCTOS EN BASE A SU DENOMINACIÓN
DELIMITER $$
CREATE FUNCTION contar_productos(denominación INT) RETURNS INT
DETERMINISTIC
BEGIN
  DECLARE total INT;
  SET total = (
    SELECT COUNT(*) 
    FROM productos 
    WHERE productos.denominación = denominación);
  RETURN total;
END
$$

DELIMITER ;
SELECT contar_productos(6) AS cantidad_productos;

# 3 - NOMBRE DE LOS INSUMOS UTILIZADOS POR CADA APARADOR
DELIMITER //
CREATE FUNCTION insumos_nombre (p_id_insumos INT) RETURNS VARCHAR(20)
READS SQL DATA
BEGIN
	DECLARE resultado VARCHAR(20);
    SET resultado = (SELECT nombre
					 FROM insumos
					 WHERE id_insumos = p_id_insumos);
	RETURN resultado;
END //
DELIMITER ;

SELECT insumos_nombre(4) AS nombre_insumo;

# CONSULTA DINÁMICA
SELECT id_aparador, nombre, domicilio, insumos_nombre(id_aparador) AS nombre_insumo FROM aparadores;

# CREACIÓN DE STORED PROCEDURES
# 1 - SP QUE ACTUALIZA LOS PRECIOS DE LOS PRODUCTOS ENVIADOS COMO PRIMER PARÁMETRO CON EL VALOR ENVIADO COMO SEGUNDO PARÁMETRO.
DELIMITER //
CREATE PROCEDURE sp_precios_actualizados(
	IN producto_precioanterior DECIMAL(6, 2),
    IN producto_precionuevo DECIMAL(6, 2))
BEGIN
	UPDATE productos 
    SET precio_unitario = producto_precionuevo
	WHERE precio_unitario = producto_precioanterior;
 END //
DELIMITER ;

SET sql_safe_updates=0;

CALL sp_precios_actualizados(1800, 2200);
SELECT * FROM productos;

# 2 - SP QUE INDICANDO EL SECTOR AL CUAL PERTENECE, RETORNA EL PROMEDIO DE SUELDO DE LOS EMPLEADOS Y EL MÁXIMO DE LOS MISMOS.
DELIMITER $$
CREATE PROCEDURE sp_sector(
	IN p_sector INT,
    OUT promedio FLOAT,
    OUT mayor FLOAT)
BEGIN
	SELECT AVG(salario) INTO promedio
    FROM empleados
    WHERE sector = p_sector;
    SELECT MAX(salario) INTO mayor
    FROM empleados
    WHERE sector = p_sector;
END $$
DELIMITER ;

CALL sp_sector(2, @p, @m);
SELECT @p, @m;

# 3 - SP QUE INSERTA EL NOMBRE DEL NUEVO INSUMO ADQUIRIDO
DELIMITER //
CREATE PROCEDURE sp_nombre_insumos(
	IN i_nombre VARCHAR(20),
    IN s_nombre INT)
BEGIN
	INSERT INTO insumos(id_insumos, nombre, sector) VALUES(NULL, i_nombre, s_nombre);
END //
DELIMITER ;

DELETE FROM sectores
WHERE id_insumos = 13;

CALL sp_nombre_insumos('cuero', '1');
SELECT * FROM insumos;

# 4 - SP QUE CUENTA LOS PRODUCTOS DENOMINADOS.
DELIMITER //
CREATE PROCEDURE sp_contar_productos_denominados(
	IN p_denominación INT,
	OUT p_total INT)
BEGIN 
	SET p_total = (SELECT COUNT(*)
				   FROM productos
				   WHERE denominación = p_denominación);
END //
DELIMITER ;

CALL sp_contar_productos_denominados(1, @p_total);
SELECT @p_total;

# CREACIÓN DE TRIGGERS
# 1 - TRIGGER DE INSERCIÓN DE REGISTROS PARA AUDITORÍA
CREATE TRIGGER tr_insert_ventas_aud
AFTER INSERT ON ventas
FOR EACH ROW
INSERT INTO AUDITS_ventas(entity, entity_id, insert_dt, created_by, last_update_dt, last_updated_by) 
VALUES ('ventas', NEW.id_ventas, CURRENT_TIMESTAMP(), USER(), CURRENT_TIMESTAMP(), USER());

INSERT INTO ventas (id_ventas, fecha, monto, cantidad, descuentos, clientes, productos, stock, viajantes) 
VALUES (NULL,'2023-06-21',930000.00,78,0,14,3,1,5);

SELECT * FROM ventas;
SELECT * FROM audits_ventas;

# 2 - TRIGGER DE ACTUALIZACIÓN DE REGISTROS PARA AUDITORÍA
CREATE TRIGGER tr_update_ventas_aud
AFTER UPDATE ON ventas
FOR EACH ROW
UPDATE audits_ventas SET last_update_dt = CURRENT_TIMESTAMP(), last_updated_by = USER() 
WHERE entity_id = OLD.id_ventas;

UPDATE ventas SET cantidad = 22 WHERE id_ventas = 13;

SELECT * FROM audits_ventas;

# 3 - TRIGGER QUE OTORGUE UN 10 % DE BONIFICACIÓN A LOS CLIENTES QUE SUPEREN LOS $400.000 EN UNA COMPRA
DELIMITER $$
CREATE TRIGGER bonificación_venta
BEFORE INSERT ON ventas
FOR EACH ROW 
BEGIN
    IF NEW.monto > 400000 THEN
	SET NEW.descuentos = 0.10;
    END IF; 
END $$
DELIMITER ;

INSERT INTO ventas(id_ventas, fecha, monto, cantidad, clientes, productos, stock, viajantes)
VALUES (NULL,'2023-02-11',450000.00,310, 5, 4, 2, 3);

SELECT * FROM ventas;
