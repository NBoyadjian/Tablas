CREATE DATABASE purinky_dw;

USE purinky_dw;

  # CREACIÓN DE TABLAS
CREATE TABLE proveedores(
	id_proveedores INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    nombre VARCHAR(30) NOT NULL, 
    apellido VARCHAR(30) NOT NULL,
    razón_social VARCHAR(45) NOT NULL,
    dni INT NOT NULL UNIQUE,
    cuit CHAR(11) NOT NULL UNIQUE,
    email VARCHAR(45),
    web VARCHAR(45),
    teléfono VARCHAR(20),
    domicilio VARCHAR(30) NOT NULL,
    localidad INT NOT NULL,
    insumos INT NOT NULL
);

CREATE TABLE insumos(
	id_insumos INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    nombre VARCHAR(30) NOT NULL,
    sector INT NOT NULL
);

CREATE TABLE empleados(
	id_empleados INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    nombre VARCHAR(30) NOT NULL,
    apellido VARCHAR(30) NOT NULL,
    dni INT NOT NULL UNIQUE,
    cuit CHAR(11) NOT NULL UNIQUE,
    salario DECIMAL(8, 2) NOT NULL,
    fecha_nacimiento DATE NOT NULL,
    teléfono VARCHAR(20) NOT NULL,
    domicilio VARCHAR(30),
    localidad INT NOT NULL,
    sector INT NOT NULL
);

CREATE TABLE productos(
	id_productos INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    denominación INT NOT NULL,
    artículo INT NOT NULL,
    precio_unitario DECIMAL(6, 2) NOT NULL,
    costo_unitario DECIMAL(6, 2) NOT NULL
);

CREATE TABLE ventas(
	id_ventas INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    fecha DATE NOT NULL,
    monto DECIMAL(10, 2) NOT NULL,
    cantidad INT NOT NULL,
	descuentos DECIMAL(3, 2) DEFAULT 0,
    clientes INT NOT NULL,
    productos INT NOT NULL,
    stock INT NOT NULL,
    viajantes INT NOT NULL
);
    
CREATE TABLE clientes(
	id_clientes INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    nombre VARCHAR(30) NOT NULL,
    apellido VARCHAR(30) NOT NULL,
    dni INT NOT NULL UNIQUE,
    fecha_nacimiento DATE NOT NULL,
    email VARCHAR(45),
    teléfono VARCHAR(30) NOT NULL,
    cuit CHAR(11) NOT NULL UNIQUE,
    domicilio VARCHAR(30) NOT NULL,
    localidad INT NOT NULL,
    viajante INT NOT NULL
);

CREATE TABLE facturación_ventas(
	id_facturación_ventas INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    cliente INT NOT NULL,
    nota_de_crédito INT DEFAULT 0,
    nota_de_débito INT DEFAULT 0,
    cheques INT DEFAULT 0,
    transacciones INT NOT NULL,
    composición_de_deuda DECIMAL(10, 2) DEFAULT 0
);

CREATE TABLE facturación_proveedores(
	id_facturación_proveedores INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    proveedor INT NOT NULL,
    pagos INT NOT NULL,
    nota_de_crédito INT DEFAULT 0,
    nota_de_débito INT DEFAULT 0,
    cheques INT DEFAULT 0,
    transacciones INT NOT NULL,
    composición_de_deuda DECIMAL(10, 2) DEFAULT 0
);

CREATE TABLE facturación_aparadores(
	id_facturación_aparadores INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    aparador INT NOT NULL,
    pagos INT NOT NULL,
    nota_de_crédito INT DEFAULT 0,
    nota_de_débito INT DEFAULT 0,
    cheques INT DEFAULT 0,
    transacciones INT NOT NULL,
    composición_de_deuda DECIMAL(10, 2) DEFAULT 0
);

CREATE TABLE aparadores(
	id_aparador INT NOT NULL AUTO_INCREMENT,
    nombre VARCHAR(30) NOT NULL,
    razón_social VARCHAR(45) NOT NULL,
    cuit CHAR(11) NOT NULL UNIQUE,
    domicilio VARCHAR(30) NOT NULL,
    localidad INT NOT NULL,
    insumos INT NOT NULL,
    PRIMARY KEY(id_aparador)
);

CREATE TABLE sectores(
	id_sector INT NOT NULL AUTO_INCREMENT,
    nombre VARCHAR(30) NOT NULL,
    PRIMARY KEY(id_sector)
);

CREATE TABLE viajantes(
	id_viajante INT NOT NULL AUTO_INCREMENT,
    nombre VARCHAR(30) NOT NULL,
    apellido VARCHAR(30) NOT NULL,
    dni INT NOT NULL UNIQUE,
    cuit CHAR(11) NOT NULL UNIQUE,
    fecha_nacimiento DATE NOT NULL,
	email VARCHAR(45),
    teléfono VARCHAR(20) NOT NULL,
    salario DECIMAL(8, 2) NOT NULL,
    domicilio VARCHAR(30) NOT NULL,
    localidad INT NOT NULL,
    zona INT NOT NULL,
    PRIMARY KEY(id_viajante)
);

CREATE TABLE stock(
	id_stock INT NOT NULL AUTO_INCREMENT,
    nombre VARCHAR(50) NOT NULL,
    PRIMARY KEY(id_stock)
);

CREATE TABLE provincia(
	id_provincia INT NOT NULL AUTO_INCREMENT,
    nombre VARCHAR(50) NOT NULL,
	PRIMARY KEY(id_provincia)
);

CREATE TABLE localidades(
	id_localidad INT NOT NULL AUTO_INCREMENT,
    nombre VARCHAR(50) NOT NULL,
    provincia INT NOT NULL,
    PRIMARY KEY(id_localidad)
);

CREATE TABLE talles(
	id_talle INT NOT NULL AUTO_INCREMENT,
    número INT NOT NULL,
    PRIMARY KEY(id_talle)
);

CREATE TABLE denominaciones(
	id_denominación INT NOT NULL AUTO_INCREMENT,
    nombre VARCHAR(30) NOT NULL,
    PRIMARY KEY(id_denominación)
);

CREATE TABLE artículos(
	id_artículo INT NOT NULL AUTO_INCREMENT,
    número VARCHAR(30) NOT NULL,
    talle INT NOT NULL,
    PRIMARY KEY(id_artículo)
);

CREATE TABLE zonas(
	id_zona INT NOT NULL AUTO_INCREMENT,
    nombre VARCHAR(50) NOT NULL,
    PRIMARY KEY(id_zona)
);

CREATE TABLE AUDITS_ventas(
	id_log INT PRIMARY KEY AUTO_INCREMENT,
    entity VARCHAR(100),
    entity_id INT,
    insert_dt DATETIME,
    created_by VARCHAR(100),
    last_update_dt DATETIME,
    last_updated_by VARCHAR(100)
);

